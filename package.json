{
	"name": "template-express",
	"version": "1.0.0",
	"main": "index.js",
	"scripts": {
		"build": "tsc",
		"serve": "nodemon src/index.ts --exec ts-node",
		"migration:generate": "npx ts-node ./node_modules/typeorm/cli.js migration:generate -n",
		"migration:run": "npx ts-node ./node_modules/typeorm/cli.js migration:run",
		"migration:revert": "npx ts-node ./node_modules/typeorm/cli.js migration:revert",
		"migration:show": "npx ts-node ./node_modules/typeorm/cli.js migration:show",
		"cache:clear": "npx ts-node ./node_modules/typeorm/cli.js cache:clear",
		"entity:create": "npx ts-node ./node_modules/typeorm/cli.js entity:create -n",
		"subscriber:create": "npx ts-node ./node_modules/typeorm/cli.js subscriber:create -n"
	},
	"keywords": [],
	"author": "Ramos Aparicio",
	"license": "ISC",
	"description": "",
	"dependencies": {
		"bcrypt": "^5.0.0",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"express-validator": "^6.6.1",
		"fs-extra": "^9.0.1",
		"helmet": "^4.1.1",
		"jsonwebtoken": "^8.5.1",
		"morgan": "^1.10.0",
		"multer": "^1.4.2",
		"reflect-metadata": "^0.1.13",
		"sqlite3": "^5.0.0",
		"swagger-jsdoc": "^4.2.0",
		"swagger-ui-express": "^4.1.4",
		"typeorm": "^0.2.28",
		"uuid": "^8.3.0"
	},
	"devDependencies": {
		"@types/bcrypt": "^3.0.0",
		"@types/compression": "^1.7.0",
		"@types/cors": "^2.8.7",
		"@types/express": "^4.17.8",
		"@types/express-validator": "^3.0.0",
		"@types/fs-extra": "^9.0.1",
		"@types/helmet": "0.0.48",
		"@types/jsonwebtoken": "^8.5.0",
		"@types/mongoose": "^5.7.36",
		"@types/morgan": "^1.9.1",
		"@types/multer": "^1.4.4",
		"@types/node": "^14.11.2",
		"@types/swagger-jsdoc": "^3.0.2",
		"@types/swagger-ui-express": "^4.1.2",
		"@types/uuid": "^8.3.0",
		"cross-env": "^7.0.2",
		"nodemon": "^2.0.4",
		"ts-node": "^9.0.0",
		"typescript": "^4.0.3"
	}
}
